<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_feed_content"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">



    <TextView
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/white"
        android:gravity="center|start"
        android:paddingLeft="17dp"
        android:text="提交成功"
        android:textColor="@color/text_color333"
        android:textSize="@dimen/textSizeXXX" />

    <TextView
        android:id="@+id/content"
        android:layout_width="280dp"
        android:layout_height="110dp"
        android:autoLink="phone"
        android:background="@color/white"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:paddingTop="15dp"
        android:text="@string/feed_back_succeed"
        android:textColor="@color/text_color999"
        android:textSize="@dimen/textSizeXXX" />

    <View
        style="@style/View_line_gray_1dp"
        android:layout_width="280dp"

        />

    <TextView
        android:id="@+id/tv_feed_ok"
        android:layout_width="280dp"
        android:layout_height="30dp"
        android:gravity="center"
        android:text="确定"
        android:textColor="@color/main_color" />


</LinearLayout>