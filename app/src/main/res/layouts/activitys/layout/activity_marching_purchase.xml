<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar_3_0" />


    <TextView
        android:id="@+id/ckwdbj"
        style="@style/text_left_name_style"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@drawable/bg_bottom_line"
        android:drawableRight="@drawable/arrow_right_eq"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:text="查看我的报价"
        android:textColor="@color/text_color333"
        android:textSize="@dimen/textSizeTitle"
        android:visibility="gone"
        tools:visibility="visible" />

    <!--item_invite_friend_list-->
    <com.hldj.hmyg.buyer.weidet.CoreRecyclerView
        android:id="@+id/recycle"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


</LinearLayout>