<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <include
        android:id="@+id/include3"
        layout="@layout/toolbar_3_0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/top_line"
        style="@style/View_line_gray"

        app:layout_constraintTop_toBottomOf="@+id/include3" />


    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/head"
        android:layout_width="72dp"
        android:layout_height="66dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="12dp"
        android:src="@drawable/icon_persion_pic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/top_line" />

    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text=""
        android:textColor="@color/text_color333"
        android:textSize="@dimen/textSizeTitle"
        app:layout_constraintStart_toEndOf="@+id/head"
        app:layout_constraintTop_toTopOf="@+id/head" />

    <com.coorchice.library.SuperTextView
        android:id="@+id/identity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:paddingLeft="25dp"
        android:textSize="@dimen/textSizeContent"
        app:isShowState="true"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="@+id/name"
        app:layout_constraintTop_toBottomOf="@+id/name"
        app:solid="@color/trans"
        app:state_drawable="@mipmap/wd_smrz_tg"
        app:state_drawable_height="17dp"
        app:state_drawable_mode="left"
        app:state_drawable_width="20dp"
        tools:text="已实名认证" />

    <com.coorchice.library.SuperTextView
        android:id="@+id/gys"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="0dp"
        android:paddingLeft="25dp"
        android:textSize="@dimen/textSizeContent"
        app:isShowState="true"
        app:layout_constraintStart_toEndOf="@+id/identity"
        app:layout_constraintTop_toTopOf="@+id/identity"
        app:solid="@color/trans"
        app:state_drawable="@mipmap/wd_gys_lv2"
        app:state_drawable_height="23dp"
        app:state_drawable_mode="left"
        app:state_drawable_width="23dp"
        tools:text="铜牌供应商ddddddd" />

    <CheckedTextView
        android:id="@+id/attention"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:layout_marginRight="12dp"
        android:background="@drawable/bg_check_gray_uncheck_main_selecteor"
        android:gravity="center"
        android:paddingLeft="3dp"
        android:paddingTop="5dp"
        android:paddingRight="3dp"
        android:paddingBottom="5dp"
        android:text="+关注"
        android:textAlignment="center"
        android:textColor="@color/color_check_gray_uncheck_main_selecteor"
        android:textSize="@dimen/textSizeContent"
        app:layout_constraintBottom_toTopOf="@+id/gys"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/head"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView46"
        style="@style/View_line_gray"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/head" />


    <TextView
        android:id="@+id/textView47"
        style="@style/View_line_gray"
        android:layout_marginTop="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/zypz_content" />

    <TextView
        android:id="@+id/textView48"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="12dp"
        android:text="Ta的店铺"
        android:textSize="@dimen/textSizeContent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView46" />

    <TextView
        android:id="@+id/store_name"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:drawableRight="@drawable/arrow_right_eq"
        android:gravity="center_vertical"
        android:paddingRight="8dp"
        android:textColor="@color/text_color333"
        android:textSize="@dimen/textSizeContent"
        app:layout_constraintBottom_toBottomOf="@+id/textView48"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/name"
        app:layout_constraintTop_toTopOf="@+id/textView48"
        tools:text="盛达花木场" />


    <TextView
        android:id="@+id/line_zypz"
        style="@style/View_line_gray"
        android:layout_height="1dp"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView48" />


    <TextView
        android:id="@+id/zypz"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:text="主营品种"
        android:textSize="@dimen/textSizeContent"
        app:layout_constraintBottom_toTopOf="@+id/textView47"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line_zypz" />

    <TextView
        android:id="@+id/zypz_content"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:gravity="center_vertical"
        android:minHeight="40dp"
        android:paddingRight="8dp"
        android:textColor="@color/text_color333"
        android:textSize="@dimen/textSizeContent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="@+id/name"
        app:layout_constraintTop_toBottomOf="@+id/line_zypz"
        tools:text="香画香樟，白激动画" />


    <TextView
        android:id="@+id/textView50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="12dp"
        android:text="苗木圈"
        android:textSize="@dimen/textSizeContent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView47" />

    <TextView
        android:id="@+id/moment_cycle"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:drawableRight="@drawable/arrow_right_eq"
        android:gravity="center_vertical"
        android:paddingRight="8dp"
        android:textColor="@color/main_color"
        android:textSize="@dimen/textSizeContent"
        app:layout_constraintBottom_toBottomOf="@+id/textView50"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/name"
        app:layout_constraintTop_toTopOf="@+id/textView50"
        tools:text="暂无苗木圈动态" />


    <View
        android:id="@+id/textView52"
        android:layout_width="0dp"
        android:layout_height="1px"
        android:layout_marginTop="12dp"
        android:background="@color/divider_color_bg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView50" />


    <TextView
        android:id="@+id/textView53"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="12dp"
        android:text="所在地"
        android:textSize="@dimen/textSizeContent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView52" />


    <TextView
        android:id="@+id/city_name"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:gravity="center_vertical"
        android:paddingRight="8dp"
        android:textColor="@color/text_color999"
        android:textSize="@dimen/textSizeContent"
        app:layout_constraintBottom_toBottomOf="@+id/textView53"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/name"
        app:layout_constraintTop_toTopOf="@+id/textView53"
        tools:text="福建厦门" />

    <TextView
        android:id="@+id/textView55"
        android:layout_width="0dp"
        android:layout_height="1px"
        android:layout_marginTop="12dp"
        android:background="@color/divider_color_bg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView53" />


    <Button
        android:id="@+id/button_ddh"
        android:layout_width="match_parent"
        android:layout_height="@dimen/tableHeight"
        android:layout_margin="15dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/round_textview_main_color_bg"
        android:text="打电话"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView55" />


    <Button
        android:id="@+id/button_jgz"
        android:layout_width="match_parent"
        android:layout_height="@dimen/tableHeight"
        android:layout_margin="15dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/round_textview_white_bg"
        android:text="加关注"
        android:textColor="@color/main_color"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/button_ddh" />


</android.support.constraint.ConstraintLayout>