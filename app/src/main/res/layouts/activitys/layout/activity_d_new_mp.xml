<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/top_padding_statu" />

    <include layout="@layout/toolbar_3_0" />


    <TextView
        android:id="@+id/text_text"
        android:layout_width="match_parent"
        android:layout_height="0dp" />


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.hldj.hmyg.buyer.weidet.CoreRecyclerView
            android:id="@+id/recycle"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/gray_bg_ed" />


        <ImageView
            android:id="@+id/btn_jrdp"
            style="@style/iv_home_publish"
            android:layout_gravity="right|bottom"
            android:layout_marginBottom="76dp"
            android:src="@mipmap/jrdp"

            />

        <ImageView
            android:id="@+id/btn_xzmp"
            style="@style/iv_home_publish"
            android:layout_gravity="right|bottom"
            android:src="@mipmap/xzmp" />

    </FrameLayout>


    <!--<include layout="@layout/item_footer_d_new_mp" />-->
</LinearLayout>