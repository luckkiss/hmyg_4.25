<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/color_bg_f5"
    android:clickable="true"
    android:foreground="?attr/selectableItemBackground"
    android:orientation="vertical"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="25dp">

    <TextView
        android:id="@+id/tv_program_purch_sub_price_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="¥3200/株"
        android:textSize="@dimen/textSizeContent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_program_purch_sub_price_cont_serv_pric"
        android:layout_width="0dp"
        android:layout_height="18dp"
        android:layout_marginEnd="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginRight="4dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="8dp"
        android:ellipsize="end"
        android:singleLine="true"
        android:text="¥0(含服务费)"
        android:textColor="@color/text_color999"
        android:textSize="@dimen/textSizeContent"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@+id/bak"
        app:layout_constraintStart_toEndOf="@+id/tv_program_purch_sub_price_type"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_program_purch_sub_use_state"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:layout_marginRight="4dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/round_rectangle_bg_btn"
        android:gravity="center"
        android:minWidth="55dp"
        android:padding="3dp"
        android:text="采用"
        android:textColor="@color/color_purch_sub"
        android:textSize="@dimen/textSizeContent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/bak"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:background="@drawable/round_rectangle_bg_btn"
        android:padding="3dp"
        android:text="设为备选"
        android:textColor="@color/color_purch_sub"
        android:textSize="@dimen/textSizeContent"
        app:layout_constraintRight_toLeftOf="@+id/tv_program_purch_sub_use_state"
        app:layout_constraintTop_toTopOf="@+id/tv_program_purch_sub_use_state" />


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginBottom="0dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="71dp"
        android:src="@drawable/yicaiyong_selector"
        app:layout_constraintBottom_toBottomOf="@+id/tv_ygdhj"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.418" />


    <TextView
        android:id="@+id/final_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="成交单价:-"
        android:textSize="@dimen/textSizeContent"
        app:layout_constraintLeft_toLeftOf="@+id/tv_program_purch_sub_price_type"
        app:layout_constraintTop_toBottomOf="@+id/tv_program_purch_sub_price_type" />

    <TextView
        android:id="@+id/tv_program_purch_sub_suplier"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="4dp"
        android:text="花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商花木购供应商"
        android:textColor="@color/main_color"
        android:textSize="@dimen/textSizeContent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/final_price" />

    <TextView
        android:id="@+id/tv_program_purch_sub_plant_addt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="4dp"
        android:textSize="@dimen/textSizeContent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/guideline5"
        app:layout_constraintTop_toBottomOf="@+id/tv_ygdhj"
        tools:text="苗源地：福建 漳州" />

    <TextView
        android:id="@+id/tv_program_purch_sub_space_text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="4dp"
        android:textSize="@dimen/textSizeContent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_program_purch_sub_plant_addt"
        tools:text="报价说明：米径：20；。。。。。。" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="由花木易购发货"
        android:textColor="@color/text_color999"
        android:visibility="gone"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_program_purch_sub_price_cont_serv_pric" />

    <TextView
        android:id="@+id/tv_program_purch_sub_is_true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/round_rectangle_bg_is_ok"
        android:paddingBottom="2dp"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:paddingTop="2dp"
        android:textColor="@color/color_purch_sub_is_ok"
        android:textSize="@dimen/textSizeContent"

        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_program_purch_sub_space_text"
        tools:text="不合格" />

    <TextView
        android:id="@+id/tv_program_purch_sub_remark"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="6dp"
        android:textSize="@dimen/textSizeContent"
        app:layout_constraintLeft_toRightOf="@+id/tv_program_purch_sub_is_true"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_program_purch_sub_space_text"
        tools:text="一株胸径19.一株胸径19.8 一株胸径2一株胸径19.8 一株胸径2一株胸径19.8 一株胸径2一株胸径19.8 一株胸径2一株胸径19.8 一株胸径2一株胸径19.8 一株胸径28 一株胸径23" />

    <com.coorchice.library.SuperTextView
        android:id="@+id/tv_program_purch_sub_images_count"
        android:layout_width="25dp"
        android:layout_height="20dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="6dp"
        app:corner="4dp"
        app:isShowState="true"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_program_purch_sub_remark"
        app:left_bottom_corner="true"
        app:left_top_corner="true"
        app:right_bottom_corner="true"
        app:right_top_corner="true"
        app:solid="@color/trans"
        app:state_drawable="@drawable/baojia_chakantupian"
        app:state_drawable_height="16dp"
        app:state_drawable_mode="center"
        app:state_drawable_width="20dp" />

    <View
        android:id="@+id/view10"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginBottom="1dp"
        android:layout_marginTop="8dp"
        android:background="@color/gray_btn_bg_pressed_color"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_program_purch_sub_images_count"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/tv_program_purch_sub_plant_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="种植类型："
        android:visibility="gone"
        app:layout_constraintLeft_toLeftOf="@+id/guideline5"
        app:layout_constraintTop_toBottomOf="@+id/tv_program_purch_sub_suplier" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="150dp"
        tools:layout_editor_absoluteX="220dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/tv_ygdhj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="4dp"
        android:text="预估到货价："
        android:visibility="gone"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_program_purch_sub_suplier" />

    <com.coorchice.library.SuperTextView
        android:id="@+id/tv_bottom_left"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="4dp"
        android:drawablePadding="5dp"
        android:text="苗木图片：有0张图片"
        android:textSize="@dimen/textSizeContent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_program_purch_sub_remark"
        app:solid="@color/trans" />


</android.support.constraint.ConstraintLayout>